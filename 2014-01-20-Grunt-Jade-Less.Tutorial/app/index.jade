doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    title Jade Tutorial
    link(rel='stylesheet', href='/css/style.css')
    script(src='/js/vendor/jquery-1.10.2.js')
    script(src='/js/source/main.js')
  body
    header
      h1 Welcome to Jade
      img(src='http://static.ddmcdn.com/gif/socialize-bully300-breed0-1.jpg')
    #section1
      ul
        li one
        li two
        li three
      - var firstName = "Kenneth";
      h2= firstName
      - for(var i = 0; i < 100; i++)
        h3= i
        h2 world
    #section2
      h2 Section 1
        p This is paragraph 1
        p 
          | This is paragraph 2 <br>
          | Addendum <strong>to</strong> paragraph 2
          | span Span1

    footer.foot1.foot2#footerid
